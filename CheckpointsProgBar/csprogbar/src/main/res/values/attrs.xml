<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CSProgressBar">
        <attr name="csProgress" format="integer"/>
        <attr name="csSize" format="integer"/>
        <attr name="csBarHeight" format="dimension"/>
        <attr name="csFilledColor" fomrat="color"/>
        <attr name="csUnfilledColor" format="color"/>

        <attr name="csCheckpoints" format="reference"/>
        <attr name="csCheckpointColors" fomrat="reference"/>

        <attr name="csMarkers" format="reference"/>
